      "cell_type": "code",
      "source": [
        "#Task 1b\n",
        "from functools import reduce\n",
        "prices=[100,250,399,50]\n",
        "tax_rate=0.10\n",
        "\n",
        "sales_tax=list(map(lambda p:p*tax_rate,prices))\n",
        "print(\"Sales tax:\\n\", sales_tax)\n",
        "\n",
        "final_prices=list(map(lambda p:p+p*tax_rate, prices))\n",
        "print(\"Final prices:\\n\", final_prices)\n",
        "\n",
        "total_tax=reduce(lambda a,b:a+b, sales_tax)\n",
        "print(\"Total sales tax:\\n\", total_tax)"
      ],
      "metadata": {
        "colab": {
          "base_uri": "https://localhost:8080/"
        },
        "id": "W7XQn65hpeM7",
        "outputId": "94ec5bbd-a46c-4b0d-8873-05e36de063a9"
